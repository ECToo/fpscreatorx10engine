          aiactionseq(seq).value=loadinternalsoundcore(rootdir$+tfile$,1)
         endif
        endif
       endif
      endif
     endif
    endcase
    case AIACTLOOPSOUND:
     tfile$=aiactionseq(seq).filename
     if tfile$<>""
      if tfile$="$0"
       aiactionseq(seq).value=-1
      else
       if tfile$="$1"
