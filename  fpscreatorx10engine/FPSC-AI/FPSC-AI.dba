  tokay=1
  if aiindexmaster>0
   for aiindex=1 to aiindexmaster
    if scriptbank$(aiindex)=ai$ then tokay=0
   next aiindex
  endif
  if tokay=1
    `
    rem Find free space in 'scriptbank'
    freeaiindex=-1
    if aiindexmaster>0
